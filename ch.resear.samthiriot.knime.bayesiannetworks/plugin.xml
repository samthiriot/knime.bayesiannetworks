<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
                            <extension
                                  point="org.knime.workbench.repository.categories">
                               <category
                                     description="Bayesian networks"
                                     icon="src/ch/resear/thiriot/knime/bayesiannetworks/bn-icon.png"
                                     level-id="bayesiannet"
                                     locked="false"
                                     name="Bayesian networks"
                                     path="/community">
                               </category>
                               <category
                                     description="Read Bayesian networks"
                                     icon="src/ch/resear/thiriot/knime/bayesiannetworks/reader/bnreader-icon.png"
                                     level-id="read"
                                     locked="false"
                                     name="Read"
                                     path="/community/bayesiannet">
                               </category>
                               <category
                                     after="read"
                                     icon="src/ch/resear/thiriot/knime/bayesiannetworks/sample/bnsample-icon.png"
                                     level-id="sample"
                                     locked="false"
                                     name="Sample"
                                     path="/community/bayesiannet">
                               </category>
                               <category
                                     after="sample"
                                     icon="src/ch/resear/thiriot/knime/bayesiannetworks/bn-icon.png"
                                     level-id="learn"
                                     locked="false"
                                     name="Learn"
                                     path="/community/bayesiannet">
                               </category>
                               <category
                                     after="learn"
                                     description="Write Bayesian networks"
                                     icon="src/ch/resear/thiriot/knime/bayesiannetworks/writer/bnwriter-icon.png"
                                     level-id="write"
                                     locked="false"
                                     name="Write"
                                     path="/community/bayesiannet">
                               </category>
                            </extension>
   <extension point="org.knime.core.PortType">
      <portType
            color="#219829"
            hidden="false"
            name="ch.res_ear.samthiriot.porttype.bayesiannet"
            objectClass="ch.resear.thiriot.knime.bayesiannetworks.port.BayesianNetworkPortObject"
            objectSerializer="ch.resear.thiriot.knime.bayesiannetworks.port.BayesianNetworkPortObject$Serializer"
            specClass="ch.resear.thiriot.knime.bayesiannetworks.port.BayesianNetworkPortSpec"
            specSerializer="ch.resear.thiriot.knime.bayesiannetworks.port.BayesianNetworkPortSpec$Serializer">
      </portType>
   </extension>

   
   <extension
         point="org.knime.workbench.repository.nodes">
      <node category-path="/community/bayesiannet/read" factory-class="ch.resear.thiriot.knime.bayesiannetworks.reader.XMLBIFBNReaderNodeFactory" id="ch.res_ear.samthiriot.gosp.bayesiannet.reader.XMLBIFBNReaderNodeFactory"/>
   <node category-path="/community/bayesiannet/sample" factory-class="ch.resear.thiriot.knime.bayesiannetworks.sample.SampleFromBNNodeFactory" id="ch.res_ear.samthiriot.gosp.bayesiannet.sample.SampleFromBNNodeFactory"/>
<node category-path="/community/bayesiannet/learn" factory-class="ch.resear.thiriot.knime.bayesiannetworks.learn.LearnBNFromSampleNodeFactory" id="ch.res_ear.samthiriot.gosp.bayesiannet.learn.LearnBNFromSampleNodeFactory"/>
<node category-path="/community/bayesiannet/write" factory-class="ch.resear.thiriot.knime.bayesiannetworks.writer.BNXMLNBIFWriterNodeFactory" id="ch.res_ear.samthiriot.gosp.bayesiannet.writer.BNXMLNBIFWriterNodeFactory"/>
<node category-path="/community/bayesiannet/sample" factory-class="ch.resear.thiriot.knime.bayesiannetworks.augment.AugmentSampleWithBNNodeFactory" id="ch.res_ear.samthiriot.gosp.bayesiannet.augment.AugmentSampleWithBNNodeFactory"/>

   </extension>
   
</plugin>
